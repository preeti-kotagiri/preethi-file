    "def evenlistinnumber(n):\n",
    "    r=0\n",
    "    lst=[]\n",
    "    while (n!=0):\n",
    "        r=n%10\n",
    "        if(r%2==0):\n",
    "            lst.append(r)\n",
    "        n=n//10\n",
    "    lst.reverse()\n",
    "    print(lst)\n",
    "n=int(input(\"enter a number\"))\n",
    "evenlistinnumber(n)"
   ]
